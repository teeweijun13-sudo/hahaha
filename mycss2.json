{
  "exportedMediaSourceDataList": {
    "mediaSources": [
      {
        "factoryId": "web-selector",
        "version": 2,
        "arguments": {
          "name": "295k动漫",
          "description": "采集295k动漫网站视频",
          "iconUrl": "https://www.857yhw.com/857.png",
          "searchConfig": {
            "searchUrl": "https://www.295k.cc/search/-------------.html?wd={keyword}",
            "searchUseOnlyFirstWord": true,
            "searchRemoveSpecial": true,
            "searchUseSubjectNamesCount": 1,
            "rawBaseUrl": "https://www.295k.cc",
            "requestInterval": 3000,

            "subjectFormatId": "indexed",
            "selectorSubjectFormatIndexed": {
              "selectNames": ".module-card-item .module-card-item-title a",
              "selectLinks": ".module-card-item a.module-card-item-poster",
              "selectCovers": ".module-card-item img[data-original]",
              "preferShorterName": true
            },

            // 直接进入第一个播放链接，跳过频道选择
            "channelFormatId": "no-channel",
            "selectorChannelFormatNoChannel": {
              "selectEpisodes": ".module-play-list-link:first-child",
              "selectEpisodeLinks": "",
              "matchEpisodeSortFromName": ".*"
            },

            "defaultResolution": "1080P",
            "defaultSubtitleLanguage": "CHS",
            "onlySupportsPlayers": [],
            "filterByEpisodeSort": false,
            "filterBySubjectName": false,

            "selectMedia": {
              "distinguishSubjectName": false,
              "distinguishChannelName": false
            },

            "matchVideo": {
              "enableNestedUrl": true,
              "matchNestedUrl": "^.+(m3u8\\.php|player\\.php).+",
              "matchVideoUrl": "(m3u8\\.php\\?url=|url=[^&]+|player_aaaa.*?\"url\":\"([^\"]+)\")",
              "cookies": "",
              "addHeadersToVideo": {
                "referer": "https://www.295k.cc",
                "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
              }
            }
          },
          "tier": 0
        }
      }
    ]
  }
}
